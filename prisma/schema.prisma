generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:isajrqwoihjwaor@mariadb:3306/db"
}

model announcements {
  announcements_id Int      @id @default(autoincrement())
  title            String   @db.VarChar(50)
  description      String   @db.VarChar(200)
  date             DateTime @db.DateTime(0)
}

model calendar {
  calendar_id Int      @id @default(autoincrement())
  date        DateTime @db.DateTime(0)
  start_time  String   @map("start-time") @db.VarChar(20)
  end_time    String   @map("end-time") @db.VarChar(20)
  facilitator String   @db.VarChar(45)
  room        String   @db.VarChar(20)
  stat        Int      @db.TinyInt
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model openlabGuidelines {
  filename String @db.VarChar(50)

  @@ignore
}

model users {
  email      String    @id @db.VarChar(50)
  firstName      String    @db.VarChar(50)
  lastName      String    @db.VarChar(50)
  isAdmin    Boolean   @default(false)
  eligibleAdmin Boolean @default(false)
  logoutTime DateTime? @db.DateTime(0)
}
